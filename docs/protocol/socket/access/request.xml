<user_node>
	if a parameter's length is not fixed,you should write the
	length of the parameter before you write the value of the parameter.
</user_node>
<so_header info="the socket message header">
	<framehead type="int32" length="4 bytes">
		0xabef0101
	</framehead>
	<length type="int32" length="4 bytes" />
	<timestamp type="int64 " length="8 bytes" />
	<sessionid type="int64" length="8 bytes">
		-1
	</sessionid>
	<msgType type="int32" length="4 byte" info="access in request msg">
		0
	</msgType>
	<serviceType type="int32" length="4 byte">
		-1
	</serviceType>
	<subServiceType type="int32" length="4 byte">
		-1
	</subServiceType>
	<priority type="int32" length="4 byte">
		0
	</priority>
	
	
	<attachment info="each attachment has a key and a value">
		<key>"userid"</key>
		<value/>
	</attachment>
	
	<attachment info="each attachment has a key and a value">
		<key mode="encrypted in aes">"token"</key>
		<value/>
	</attachment>
</so_header>

